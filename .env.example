# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Cohere AI
COHERE_API_KEY=your_cohere_api_key
# Chat model: c4ai-aya-expanse-32b (C4AI Vision AYA 32B), command-r7b-12-2024, command-r-plus
COHERE_CHAT_MODEL=c4ai-aya-expanse-32b
# Embeddings: Using embed-english-v4.0 per user's Railway config
COHERE_EMBED_MODEL=embed-english-v4.0
# Reranking: Using rerank-v4.0 per user's Railway config
COHERE_RERANK_MODEL=rerank-v4.0

# Stripe
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret

# Cloudflare Turnstile (optional - for bot protection)
TURNSTILE_SECRET_KEY=your_cloudflare_turnstile_secret

# Upstash Redis (optional - for rate limiting)
UPSTASH_REDIS_REST_URL=your_upstash_url
UPSTASH_REDIS_REST_TOKEN=your_upstash_token

# App Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
