# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Cohere AI
COHERE_API_KEY=your_cohere_api_key
# For C4AI Vision models, use: c4ai-aya-expanse-32b or c4ai-aya-expanse-8b
# For Cohere models, use: command-r-plus, command-r, command-r7b-12-2024, etc.
COHERE_CHAT_MODEL=command-r7b-12-2024
# For embeddings, use: embed-english-v3.0, embed-multilingual-v3.0, embed-english-light-v3.0
COHERE_EMBED_MODEL=embed-english-v3.0
# For reranking, use: rerank-v3.5, rerank-english-v3.0, rerank-multilingual-v3.0
COHERE_RERANK_MODEL=rerank-v3.5

# Stripe
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret

# Cloudflare Turnstile (optional - for bot protection)
TURNSTILE_SECRET_KEY=your_cloudflare_turnstile_secret

# Upstash Redis (optional - for rate limiting)
UPSTASH_REDIS_REST_URL=your_upstash_url
UPSTASH_REDIS_REST_TOKEN=your_upstash_token

# App Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
