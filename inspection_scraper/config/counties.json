{
  "washtenaw": {
    "name": "Washtenaw County",
    "state": "Michigan",
    "scraper_type": "sword_solutions",
    "url": "https://swordsolutions.com/inspections/",
    "county_value": "MI - Washtenaw",
    "enabled": true,
    "settings": {
      "delay": 3,
      "max_pages": 20,
      "timeout": 30,
      "retry_attempts": 3
    },
    "pagination": {
      "type": "link",
      "selector": "a.next, a[rel='next'], .pagination .next"
    },
    "selectors": {
      "row_selector": ".inspection-row, table tbody tr, div.result-item",
      "fields": {
        "business_name": ".business-name, .name, .establishment, td.name, td:nth-child(1), h3, h4",
        "address": ".address, .location, td.address, td:nth-child(2)",
        "inspection_date": ".date, .inspection-date, td.date, td:nth-child(3)",
        "violations": ".violations, .findings, td.violations",
        "severity": ".severity, .priority, .risk, td.severity",
        "report_link": "a.report, a.pdf, a[href*='.pdf'], a[href*='report']",
        "license_number": ".license, td.license",
        "type": ".type, .category"
      }
    },
    "form_fields": {
      "county": "county",
      "restaurant_name": "restaurant_name",
      "address": "address",
      "city": "city",
      "license_num": "license_num",
      "from_date": "from_date",
      "to_date": "to_date",
      "show_partial": "show_partial",
      "page": "page",
      "limit": "limit"
    },
    "api_endpoints": [
      "/api/inspections/search",
      "/api/search",
      "/inspections/api/search",
      "/inspections/search"
    ]
  },
