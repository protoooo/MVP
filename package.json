{
  "name": "protocollm",
  "version": "2.0.0",
  "description": "Unlimited intelligent document storage with powerful semantic search and retrieval",
  "main": "backend/dist/server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon backend/src/server.ts",
    "dev:frontend": "next dev",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "tsc -p backend/tsconfig.json",
    "build:frontend": "next build",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "node backend/dist/server.js",
    "start:frontend": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit && tsc -p backend/tsconfig.json --noEmit",
    "clean": "rm -rf .next backend/dist node_modules/.cache",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "keywords": [
    "document-storage",
    "semantic-search",
    "cohere",
    "unlimited-storage",
    "supabase",
    "pgvector",
    "ocr",
    "document-intelligence"
  ],
  "author": {
    "name": "Austin Northrup",
    "email": "austinnorthrop@gmail.com",
    "url": "https://protocollm.org"
  },
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/supabase-js": "^2.39.3",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.6",
    "@types/pdf-parse": "^1.1.4",
    "@types/pg": "^8.11.10",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "bcrypt": "^5.1.1",
    "clsx": "^2.1.1",
    "cohere-ai": "^7.20.0",
    "cors": "^2.8.5",
    "date-fns": "^3.0.6",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.1.5",
    "framer-motion": "^11.0.3",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.453.0",
    "mammoth": "^1.11.0",
    "multer": "^1.4.5-lts.1",
    "next": "^15.1.3",
    "pdf-parse": "^1.1.1",
    "pdf2pic": "^3.1.3",
    "pg": "^8.13.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.5",
    "resend": "^6.6.0",
    "stripe": "^20.1.0",
    "tesseract.js": "^5.1.1",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.11",
    "@types/node": "^20.10.6",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.1.2",
    "eslint": "^8.56.0",
    "eslint-config-next": "^15.1.3",
    "nodemon": "^3.1.9",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.19",
    "ts-node": "^10.9.2"
  },
  "nodemonConfig": {
    "watch": [
      "backend/src"
    ],
    "ext": "ts",
    "ignore": [
      "backend/dist/**/*",
      "node_modules/**/*"
    ],
    "exec": "ts-node backend/src/server.ts",
    "env": {
      "NODE_ENV": "development"
    }
  }
}
