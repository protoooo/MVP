{
  "name": "compliance-hub",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "clean": "rm -rf .next node_modules/.cache",
    "prebuild": "npm run clean",
    "build": "next build",
    "start": "next start -H 0.0.0.0 -p ${PORT:-3000}",
    "lint": "next lint",
    "ingest": "node scripts/ingest-documents.js",
    "ingest:local": "node --env-file=.env.local scripts/ingest-documents.js",
    "test": "echo 'No tests yet' && exit 0",
    "postbuild": "echo 'Build complete. Ready for deployment.'",
    "health": "node -e \"fetch('http://localhost:3000/api/health').then(r=>r.json()).then(console.log).catch(console.error)\""
  },
  "dependencies": {
    "@google/generative-ai": "^0.17.1",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@react-three/postprocessing": "^2.16.2",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.87.0",
    "@types/three": "^0.181.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.6.1",
    "google-auth-library": "^10.5.0",
    "lucide-react": "^0.263.1",
    "next": "14.2.5",
    "node-fetch": "^3.3.2",
    "openai": "^6.10.0",
    "pdf-parse": "^1.1.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "stripe": "^14.10.0",
    "three": "^0.181.2",
    "vanta": "^0.5.24"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4"
  }
}
